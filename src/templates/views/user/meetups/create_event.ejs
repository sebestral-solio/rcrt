        <style>
            .card {
                background-color: rgb(241, 234, 255);
                border: 1.5px solid rgb(216, 216, 216);
                border-radius: 10px;
            }

            .card-header {
                font-size: 1.3rem;
                font-weight: bolder;
                background: transparent;
                border-radius: 10px;
                color: grey;
            }

            .card:hover {
                cursor: pointer;
                border: 2px solid rgb(66, 54, 120);
            }

            .content {
                overflow: auto;
            }

            label {
                margin-bottom: 1%;
                font-size: medium;
                font-weight: bold;
            }

            p {

                font-size: small;
            }

            .form-control {
                margin-bottom: 1%;

                border-radius: 5px;
            }

            .custom-file-upload {
                font-size: small;
                display: inline-block;
                padding: 3px 5px;
                cursor: pointer;
                color: black;
                background-color: transparent;
                border: 1px solid rgb(66, 54, 120);
                border-radius: 30px;
                transition: background-color 0.3s ease;
            }

            .custom-file-upload:hover {
                color: white;
                background-color: rgb(66, 54, 120);
            }

            .custom-file-upload input[type="file"] {
                display: none;
            }



            .preview-section {
                border: 1.5px solid rgb(216, 216, 216);
                border-radius: 25px;
                background-color: white;
                padding: 1rem 1rem 0.5rem 1rem;
            }

            .preview-section img {
                border: 2px solid rgb(217, 217, 217);
                border-radius: 20px;
            }

            .price-seats {
                display: flex;
            }

            .price-seats .preview-link {
                margin-left: auto;
            }

            .is-invalid {
                border-color: #dc3545;
            }

            .is-invalid:focus {
                box-shadow: 0 0 0 0.2rem rgba(220, 53, 69, 0.25);
            }

            .marks {
                font-weight: lighter;
                color: red;
            }

            #event-title-preview {
                font-weight: bolder;
            }

            .save {
                background-color: rgb(66, 54, 120);
                transition: transform 0.3s ease-in-out, background-color 0.3s ease-in-out;
            }

            .save:hover {
                background-color: rgb(66, 54, 120);
                transform: scale(1.1);
            }

            .prev {
                color: black;
                background-color: transparent;
                border: 1px solid rgb(66, 54, 120);
            }

            .prev:hover {
                color: white;
                background-color: rgb(66, 54, 120);
            }

            .organizer-details-container {
                border: 1.5px solid #ccc;
                padding: 20px;
                border-radius: 15px;
                margin-bottom: 20px;
            }

            .organizer-details-container h4 {
                font-weight: bold;
                margin-top: 0;
                margin-bottom: 20px;
                text-align: center;
            }
        </style>

    </head>

