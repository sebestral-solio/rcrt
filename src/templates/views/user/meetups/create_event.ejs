                <main class="content">
                    <div class="container-fluid p-0">
                        <div class="multi-step-form">
                            <!-- Single Form -->
                                 <form id="event-creation-form" action="/create_event" method="POST" enctype="multipart/form-data">
                            <!-- Step 1: Build Event -->
                            <div id="step-1" class="step active">
                                <h1 class="step-heading">Step 1: Build Your Event</h1>
                                <hr class="mb-4">
                                <div id="required-error"
                                    style="color: red; display: none; margin-bottom: 1%; font-weight: bold; text-align: center;">
                                    All Fields Are required.</div>
                                <div class="container">
                                    <div class="row">
                                        <div class="col-12 mb-1">
                                            <div class="card" id="card-event-overview">
                                                <div class="card-header" id="btn-event-overview">
                                                    Event Overview
                                                    <span class="float-end" id="toggle-event-overview">
                                                        <i class="fas fa-plus"></i>
                                                    </span>
                                                </div>
                                                <div class="card-body custom-section" id="section-event-overview"
                                                    style="display: none;">
                                                    <div id="event-overview-form">
                                                        <div class="form-group mb-4">
                                                            <label for="event_title">Event Title <span
                                                                    class="marks">*</span></label>
                                                            <p>Be Clear with your event title.</p>
                                                            <input type="text" class="form-control" id="event_title"
                                                                name="event_title" placeholder="Enter event title"
                                                                maxlength="25" required oninput="updatePreview()">
                                                        </div>
                                                        <div class="form-group mb-4">
                                                            <label for="event_description">Event Description <span
                                                                    class="marks">*</span></label>
                                                            <p>Give short description that grab people's attention.</p>
                                                            <textarea class="form-control" id="event_description"
                                                                name="event_description"
                                                                placeholder="Enter event description" maxlength="140"
                                                                required></textarea>
                                                        </div>
                                                        <div class="form-group mb-4">
                                                            <label for="event_poster">Event Poster <span
                                                                    class="marks">*</span>
                                                            </label>&nbsp;&nbsp;
                                                            <label class="custom-file-upload"> Upload
                                                                <input type="file" class="form-control-file"
                                                                    id="event_poster" name="event_poster"
                                                                    accept="image/*" onchange="previewImage(event)"
                                                                    required>
                                                            </label>
                                                            <div id="poster-preview"></div>
                                                        </div>
                                                    </div>    <!-------------->
                                                </div>
                                            </div>
                                        </div>
                                        <div class="col-12 mb-1">
                                            <div class="card" id="card-date-time-location">
                                                <div class="card-header" id="btn-date-time-location">
                                                    Event Schedule
                                                    <span class="float-end" id="toggle-date-time-location">
                                                        <i class="fas fa-plus"></i>
                                                </div>
                                                <div class="card-body custom-section" id="section-date-time-location"
                                                    style="display: none;">
                                                    <div id="date-time-location-form">
                                                        <div class="form-group">
                                                            <label for="event_date">Event Date <span
                                                                    class="marks">*</span></label>
                                                            <input type="date" class="form-control" id="event_date"
                                                                name="event_date" required oninput="updatePreview()">
                                                        </div>
                                                        <div class="form-group">
                                                            <label for="start_time">Start Time <span
                                                                    class="marks">*</span></label>
                                                            <input type="time" class="form-control" id="start_time"
                                                                name="start_time" required oninput="updatePreview()">
                                                        </div>
                                                        <div class="form-group">
                                                            <label for="end_time">End Time <span
                                                                    class="marks">*</span></label>
                                                            <input type="time" class="form-control" id="end_time"
                                                                name="end_time" required oninput="updatePreview()">
                                                        </div>
                                                        <div class="form-group mb-3">
                                                            <label>Schedule Type <span
                                                                    class="marks">*</span></label><br>
                                                            <div class="form-check form-check-inline">
                                                                <input class="form-check-input" type="radio"
                                                                    name="schedule_type" id="online" value="online"
                                                                    onchange="toggleLocationInput()">
                                                                <label class="form-check-label"
                                                                    for="online">Online</label>
                                                            </div>
                                                            <div class="form-check form-check-inline">
                                                                <input class="form-check-input" type="radio"
                                                                    name="schedule_type" id="offline" value="offline"
                                                                    onchange="toggleLocationInput()" checked>
                                                                <label class="form-check-label"
                                                                    for="offline">Offline</label>
                                                            </div>
                                                        </div>
                                                        <div class="location-group">
                                                            <label for="event_address">Address<span
                                                                    class="marks">*</span></label>
                                                            <input type="text" class="form-control" id="event_address"
                                                                name="event_address" placeholder="Enter event address"
                                                                required>
                                                        </div>
                                                        <div class="location-group">
                                                            <label for="event_city">City<span
                                                                    class="marks">*</span></label>
                                                            <input type="text" class="form-control" id="event_city"
                                                                name="event_city" placeholder="Enter event city"
                                                                required>
                                                        </div>
                                                        <div class="location-group">
                                                            <label for="event_state">State<span
                                                                    class="marks">*</span></label>
                                                            <input type="text" class="form-control" id="event_state"
                                                                name="event_state" placeholder="Enter event state"
                                                                required>
                                                        </div>
                                                        <div class="location-group">
                                                            <label for="event_pincode">Pincode<span
                                                                    class="marks">*</span></label>
                                                            <input type="text" class="form-control" id="event_pincode"
                                                                name="event_pincode" placeholder="Enter event pincode"
                                                                pattern="\d*" title="Please enter only numbers"
                                                                required>
                                                        </div>
                                                    </div> <!-------------->
                                                </div>
                                            </div>
                                        </div>
                                        <div class="col-12 mb-1">
                                            <div class="card" id="card-about-event">
                                                <div class="card-header" id="btn-about-event">
                                                    Event Agenda
                                                    <span class="float-end" id="toggle-about-event">
                                                        <i class="fas fa-plus"></i>
                                                </div>
                                                <div class="card-body custom-section" id="section-about-event"
                                                    style="display: none;">
                                                    <div id="about-event-form">
                                                        <div class="form-group">
                                                            <p>Please share a brief overview and the planned activities
                                                                for this event. <span class="marks">*</span></p>
                                                            <textarea class="form-control" id="event_agenda"
                                                                name="event_agenda" style="height: 150px;"
                                                                required></textarea>
                                                        </div>
                                                    </div>   <!-------------->
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="button-group mt-4">
                                        <button type="button" class="btn btn-primary btn-step float-end save"
                                            onclick="validateStep1()">Save & Continue</button>
                                    </div>
                                </div>
                            </div>

                            <!-- Step 2: Add Tickets Section -->
                            <div id="step-2" class="step">
                                <h1 class="step-heading">Step 2: Create Tickets</h1>
                                <hr class="mb-4">
                                <div id="required-error2"
                                    style="color: red; display: none; margin-bottom: 1%; font-weight: bold; text-align: center;">
                                    All Fields Are required.</div>
                                <div class="container">
                                    <div class="row">
                                        <div class="col-12 mb-1">
                                            <div class="card" id="card-event-overview">
                                                <div class="card-header" id="btn-event-ticket">
                                                    Create Your Own
                                                    <span class="float-end" id="toggle-event-ticket">
                                                        <i class="fas fa-plus"></i>
                                                    </span>
                                                </div>
                                                <div class="card-body custom-section" id="section-event-ticket"
                                                    style="display: none;">
                                                    <div id="event-ticket-form">
                                                        <div class="form-group mb-4">
                                                            <label for="ticket_title">Ticket Name <span
                                                                    class="marks">*</span></label>
                                                            <input type="text" class="form-control" id="ticket_title"
                                                                name="ticket_title" placeholder="Enter ticket name"
                                                                maxlength="25" required>
                                                        </div>
                                                        <div class="form-group mb-4">
                                                            <label for="ticket_description">Ticket Description <span
                                                                    class="marks">*</span></label>
                                                            <p>Describe the benefits or features included with this
                                                                ticket.</p>
                                                            <textarea class="form-control" id="ticket_description"
                                                                name="ticket_description"
                                                                placeholder="Enter ticket features" maxlength="140"
                                                                required></textarea>
                                                        </div>
                                                        <div class="form-group mb-3">
                                                            <label>Ticket Type <span class="marks">*</span></label><br>
                                                            <div class="form-check form-check-inline">
                                                                <input class="form-check-input" type="radio"
                                                                    name="ticket_type" id="free" value="free"
                                                                    onchange="toggleTicketInput()">
                                                                <label class="form-check-label" for="free">Free</label>
                                                            </div>
                                                            <div class="form-check form-check-inline">
                                                                <input class="form-check-input" type="radio"
                                                                    name="ticket_type" id="paid" value="paid"
                                                                    onchange="toggleTicketInput()" checked>
                                                                <label class="form-check-label" for="paid">Paid</label>
                                                            </div>
                                                        </div>
                                                        <div class="form-group mb-4" id="ticketPrice">
                                                            <label for="ticket_price">Ticket Price <span
                                                                    class="marks">*</span></label>
                                                            <input type="number" class="form-control" id="ticket_price"
                                                                name="ticket_price"
                                                                placeholder="Enter ticket price in Rs"
                                                                oninput="updatePreview()" value="0">
                                                        </div>
                                                        <div class="form-group mb-4">
                                                            <label for="ticket_quantity">Available Quantity <span
                                                                    class="marks">*</span></label>
                                                            <input type="number" class="form-control"
                                                                id="ticket_quantity" name="ticket_quantity"
                                                                placeholder="Enter tickets Quantity"
                                                                oninput="updatePreview()" required>
                                                        </div>
                                                        <div class="sales">
                                                            <div class="form-group mb-3">
                                                                <label for="ticket_date">Sales Start Date <span
                                                                        class="marks">*</span></label>
                                                                <input type="date" class="form-control" id="ticket_date"
                                                                    name="ticket_date" required>
                                                            </div>
                                                            <div class="form-group">
                                                                <label for="ticket_end_date">Sales End Date <span
                                                                        class="marks">*</span></label>
                                                                <input type="date" class="form-control"
                                                                    id="ticket_end_date" name="ticket_end_date"
                                                                    required>
                                                            </div>
                                                        </div>
                                                        <br>
                                                        <div id="date-error"
                                                            style="color: red; display: none; font-weight: bold;">Enter
                                                            Valid Start and End Dates For Ticket Sales.</div>

                                                    </div> <!-------------->
                                                </div>
                                            </div>
                                        </div>

                                    </div>
                                    <button type="button" class="btn btn-outline-primary btn-step float-start prev"
                                        onclick="prevStep('#step-2', '#step-1')">Previous</button>
                                    <button type="button" class="btn btn-primary btn-step float-end save"
                                        onclick="validateStep2()">Save & Continue</button>
                                </div>
                            </div>
                            <!-- Step 3: Publish Event Section -->
                            <div id="step-3" class="step">
                                <h1 class="step-heading">Step 3: Ready To Host</h1>
                                <hr class="mb-4">
                                <div id="required-error3"
                                    style="color: red; display: none; margin-bottom: 1%; font-weight: bold; text-align: center;">
                                    All Fields Are required.</div>
                                <div id="publish-event-form">
                                    <div class="preview-section mb-4">
                                        <div class="row">
                                            <div class="col-md-4">
                                                <img id="event-poster-preview" src="images/avatars/profile.png"
                                                    alt="Event Poster" class="img-fluid"
                                                    style="max-height: 150px; width: 500px;">
                                            </div>
                                            <div class="col-md-8 mt-3">
                                                <h2 id="event-title-preview"></h2>
                                                <p id="event-time-preview"></p>
                                                <p id="event-location-preview"></p>
                                                <div id="price-seats" class="price-seats"></div>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="organizer-details-container">
                                        <h4>Organizer Details</h4>
                                        <div class="form-group mb-4">
                                            <label for="organization_name">Organization Name <span
                                                    class="marks">*</span></label>
                                            <input type="text" class="form-control" id="organization_name"
                                                name="organization_name" placeholder="Enter Organization name"
                                                maxlength="25" required>
                                        </div>
                                        <div class="form-group mb-4">
                                            <label for="organizer_name">Organized By <span
                                                    class="marks">*</span></label>
                                            <input type="text" class="form-control" id="organizer_name"
                                                name="organizer_name" placeholder="Enter Organizer name" maxlength="25"
                                                required>
                                        </div>
                                        <div class="form-group mb-2">
                                            <label for="organizer_contact">Contact <span class="marks">*</span></label>
                                            <input type="text" class="form-control" id="organizer_contact"
                                                name="organizer_contact" placeholder="Enter Contact Number"
                                                pattern="\d*" maxlength="13" required>
                                        </div>
                                        <div class="form-group mb-2 form-check">
                                            <input type="checkbox" class="form-check-input" id="contact_visibility" name="contact_visibility" value="1">
                                            <label class="form-check-label" for="contact_visibility" style="font-weight: lighter; font-size: smaller;">Make contact public</label>
                                        </div>
                                        
                                    </div>

                                    <div class="form-row mb-4">
                                        <div class="col mb-4">
                                            <label for="event_type">Event Type <span class="marks">*</span></label>
                                            <select class="form-control" id="event_type" name="event_type" required>
                                                <option value="" class="not-working">Select Event Type</option>
                                                <option value="conference">Conference</option>
                                                <option value="workshop">Workshop</option>
                                                <option value="seminar">Seminar</option>
                                                <option value="convention">Convention</option>
                                                <option value="fest">Festival</option>
                                                <option value="concert">Concert or Performance</option>
